{
  "name": "@hunthub/api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": "22.13.1"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/server.js",
    "type-check": "tsc --noEmit",
    "dev": "ts-node-dev --no-deprecation --respawn --transpile-only src/server.ts",
    "start:dev": "npm run type-check  --watch & npm run dev",
    "format": "prettier --write \"src/**/*.{js,ts,json,md}\"",
    "format:check": "prettier --check \"src/**/*.{js,ts,json,md}\"",
    "lint": "eslint 'src/**/*.ts'",
    "lint:fix": "eslint 'src/**/*.ts' --fix",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:coverage": "jest --coverage --runInBand",
    "init-counters": "ts-node -r tsconfig-paths/register scripts/init-counters.ts"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@hunthub/shared": "*",
    "body-parser": "^1.20.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "firebase": "^11.2.0",
    "firebase-admin": "^13.0.2",
    "inversify": "^6.2.1",
    "module-alias": "^2.2.3",
    "mongodb": "^6.12.0",
    "mongoose": "^8.9.5",
    "reflect-metadata": "^0.2.2",
    "swagger-ui-express": "^5.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^10.1.0",
    "@types/express": "^5.0.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22.10.10",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.21.0",
    "@typescript-eslint/parser": "^8.21.0",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.3",
    "jest": "^30.2.0",
    "mongodb-memory-server": "^10.2.3",
    "prettier": "^3.4.2",
    "supertest": "^7.1.4",
    "ts-jest": "^29.4.5",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.3"
  },
  "_moduleAliases": {
    "@": "src",
    "@db": "src/db"
  }
}
